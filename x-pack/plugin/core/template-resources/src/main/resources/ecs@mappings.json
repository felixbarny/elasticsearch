{
  "template": {
    "mappings": {
      "schema": "ecs",
      "dynamic_templates": [
        {
          "ecs_ip": {
            "mapping": {
              "type": "ip"
            },
            "path_match": [
              "ip",
              "*.ip",
              "*_ip"
            ],
            "match_mapping_type": "string"
          }
        },
        {
          "ecs_date": {
            "mapping": {
              "type": "date"
            },
            "path_match": [
              "*.timestamp",
              "*_timestamp",
              "*.creation_date"
            ],
            "unmatch_mapping_type": "object"
          }
        },
        {
          "ecs_geo_point": {
            "mapping": {
              "type": "geo_point"
            },
            "path_match": [
              "*.geo.location"
            ]
          }
        },
        {
          "all_strings_to_keywords": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ]
    }
  },
  "_meta": {
    "description": "dynamic mappings based on ECS, installed by x-pack",
    "managed": true
  },
  "version": ${xpack.stack.template.version},
  "deprecated": ${xpack.stack.template.deprecated}
}
